<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="shopInfo">

	<insert id="insertShopInfo" parameterType="finalWeb.command.ShopInfoCommand">
		insert into shopInfo
		values
		(#{shopInfo})
	</insert>

	<select id="selectShopInfo" resultType="finalWeb.command.ShopInfoCommand">
		select shopInfo from shopInfo
	</select>
	
	<delete id="deleteShopInfo">
		delete from shopInfo
	
	</delete>
	
</mapper>